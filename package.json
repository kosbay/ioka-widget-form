{
  "name": "zoid-lesson",
  "version": "1.0.0",
  "description": "A lesson on Zoid and its uses",
  "main": "index.js",
  "scripts": {
    "build": "concurrently --kill-others \"webpack --mode production\"",
    "b:1": "concurrently --kill-others \"webpack --mode production --env.TUTORIAL=tutorial1\"",
    "b:2": "concurrently --kill-others \"webpack -w --mode development --env.TUTORIAL=tutorial2\" \"serve ./src/tutorial2 -p 3000\"",
    "b:3": "concurrently --kill-others \"webpack -w --mode development --env.TUTORIAL=tutorial3\" \"serve ./src/tutorial3 -p 3000\"",
    "present:build": "webpack -w --mode development --env.TUTORIAL=presentation",
    "present:start": "serve ./src/presentation -p 3000",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshgreenwell/zoid-lesson.git"
  },
  "keywords": [
    "zoid",
    "xcomponent",
    "js",
    "javascript",
    "post-robot",
    "post",
    "messaging"
  ],
  "author": "Josh Greenwell <joshgreenwell99@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshgreenwell/zoid-lesson/issues"
  },
  "homepage": "https://github.com/joshgreenwell/zoid-lesson#readme",
  "dependencies": {
    "jsx-pragmatic": "^2.0.7",
    "zoid": "^9.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "concurrently": "^3.6.0",
    "jest": "^23.2.0",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.12.0",
    "serve": "^9.1.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  }
}
